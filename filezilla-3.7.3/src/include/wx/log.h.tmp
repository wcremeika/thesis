WX_DEFINE_VARARG_FUNC_NOP(wxLogSysError, 1, (const wxFormatString&))         WX_WATCOM_ONLY_CODE(                                                            WX_DEFINE_VARARG_FUNC_NOP(wxLogSysError, 1, (const char*))                   WX_DEFINE_VARARG_FUNC_NOP(wxLogSysError, 1, (const wchar_t*))                WX_DEFINE_VARARG_FUNC_NOP(wxLogSysError, 1, (const wxCStrData&))         )                                                                           inline void wxVLogSysError(const wxFormatString& WXUNUSED(format),                                     va_list WXUNUSED(argptr)) { };
WX_DEFINE_VARARG_FUNC_NOP(wxLogSysError, 2, (long, const wxFormatString&))                                                                                inline void wxVLogSysError(long WXUNUSED(arg),                                                     const wxFormatString& WXUNUSED(format),                                     va_list WXUNUSED(argptr)) {};
WX_DEFINE_VARARG_FUNC_NOP(wxLogWarning, 1, (const wxFormatString&))         WX_WATCOM_ONLY_CODE(                                                            WX_DEFINE_VARARG_FUNC_NOP(wxLogWarning, 1, (const char*))                   WX_DEFINE_VARARG_FUNC_NOP(wxLogWarning, 1, (const wchar_t*))                WX_DEFINE_VARARG_FUNC_NOP(wxLogWarning, 1, (const wxCStrData&))         )                                                                           inline void wxVLogWarning(const wxFormatString& WXUNUSED(format),                                     va_list WXUNUSED(argptr)) { };
WX_DEFINE_VARARG_FUNC_NOP(wxLogMessage, 1, (const wxFormatString&))         WX_WATCOM_ONLY_CODE(                                                            WX_DEFINE_VARARG_FUNC_NOP(wxLogMessage, 1, (const char*))                   WX_DEFINE_VARARG_FUNC_NOP(wxLogMessage, 1, (const wchar_t*))                WX_DEFINE_VARARG_FUNC_NOP(wxLogMessage, 1, (const wxCStrData&))         )                                                                           inline void wxVLogMessage(const wxFormatString& WXUNUSED(format),                                     va_list WXUNUSED(argptr)) { };
WX_DEFINE_VARARG_FUNC_NOP(wxLogInfo, 1, (const wxFormatString&))         WX_WATCOM_ONLY_CODE(                                                            WX_DEFINE_VARARG_FUNC_NOP(wxLogInfo, 1, (const char*))                   WX_DEFINE_VARARG_FUNC_NOP(wxLogInfo, 1, (const wchar_t*))                WX_DEFINE_VARARG_FUNC_NOP(wxLogInfo, 1, (const wxCStrData&))         )                                                                           inline void wxVLogInfo(const wxFormatString& WXUNUSED(format),                                     va_list WXUNUSED(argptr)) { };
