    static wxEventHashTable* sm_first;
    wxEventHashTable* m_previous;
    wxEventHashTable* m_next;

    private:                                            wxEventHashTable(const wxEventHashTable&);                    wxEventHashTable& operator=(const wxEventHashTable&);
};

