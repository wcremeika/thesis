int PWSprefs::GetConfigIndicator() const
{
  switch (m_ConfigOption) {
    case CF_NONE:
      return 3230;
    case CF_REGISTRY:
      return 3231;
    case CF_FILE_RW:
    case CF_FILE_RW_NEW:
