const TCHAR *PWSUtil::UNKNOWN_XML_TIME_STR = "1970-01-01 00:00:00";
const TCHAR *PWSUtil::UNKNOWN_ASC_TIME_STR = "Unknown";

StringX PWSUtil::ConvertToDateTimeString(const time_t &t, TMC result_format)
{
  StringX ret;
  if (t != 0) {
    TCHAR datetime_str[80];
    struct tm *st;
                      
                   
                
                                                    
                                 
                                                       
                                                   
     
    st = localtime(&t);
    if (st == NULL) // null means invalid time
      return ConvertToDateTimeString(0, result_format);
      
    switch (result_format) {
    case TMC_EXPORT_IMPORT:
      strftime(datetime_str, sizeof(datetime_str) / sizeof(datetime_str[0]),
                "%Y/%m/%d %H:%M:%S", st);
      break;
    case TMC_XML:
      strftime(datetime_str, sizeof(datetime_str) / sizeof(datetime_str[0]),
                "%Y-%m-%dT%H:%M:%S", st);
      break;
    case TMC_LOCALE:
      setlocale(LC_TIME, "");
      strftime(datetime_str, sizeof(datetime_str) / sizeof(datetime_str[0]),
                "%c", st);
      break;
    case TMC_LOCALE_DATE_ONLY:
      setlocale(LC_TIME, "");
      strftime(datetime_str, sizeof(datetime_str) / sizeof(datetime_str[0]),
                "%x", st);
      break;
    default:
      if ((asctime_r(st, datetime_str) != NULL ? 0 : 1) != 0)
        return ConvertToDateTimeString(0, result_format);
    }
    ret = datetime_str;
  } else { // t == 0
    switch (result_format) {
    case TMC_ASC_UNKNOWN:
      ret = UNKNOWN_ASC_TIME_STR;
      break;
    case TMC_XML:
      ret = UNKNOWN_XML_TIME_STR;
      break;
    default:
      ret = "";
    }
  }
  // remove the trailing EOL char.
  TrimRight(ret);
  return ret;
}

stringT PWSUtil::GetNewFileName(const stringT &oldfilename,
                                const stringT &newExtn)
{
  stringT inpath(oldfilename);
  stringT drive, dir, fname, ext;
