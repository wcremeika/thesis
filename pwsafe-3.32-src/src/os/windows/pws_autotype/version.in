#ifndef _VERSION_H_
#define _VERSION_H_

// version.h is automatically generated from version.in by a custom
// pre-build step that uses git's 'describe' function and
// replaces "GITREV" with current git commit info.

// Format: Major, Minor, Build, Revision
//   Build  = 0 for all Formally Released versions
//   Build != 0 for all Intermediate versions

#define FILEVER          1, 1, 0
#define PRODUCTVER       1, 1, 0
#define STRATFILEVER     "1, 1, 0, GITREV\0"
#define STRATPRODUCTVER  STRATFILEVER

#endif // _VERSION_H_
